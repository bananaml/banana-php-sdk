{
    "name": "banana-dev/banana-dev",
    "description": "A PHP client to interact with Banana's machine learning inference APIs",
    "keywords": [
        "Banana", "Banana.dev", "Banana client", "API wrapper", "SDK"
    ],
    "homepage": "https://github.com/bananaml/banana-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Banana.dev",
            "email": "support@banana.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "php-http/discovery": "^1.17",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^10.3.2",
        "spatie/ray": "^1.28",
        "symfony/http-client": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "BananaDev\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BananaDev\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
